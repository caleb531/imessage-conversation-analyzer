{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "imessage-conversation-analyzer",
  "version": "0.1.0",
  "identifier": "com.calebevans.imessage-conversation-analyzer",
  "build": {
    "beforeDevCommand": "vite dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "vite build",
    "frontendDist": "../build"
  },
  "app": {
    "windows": [
      {
        "title": "imessage-conversation-analyzer",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "plugins": {},
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "externalBin": [
      "ica-sidecar"
    ],
    "resources": [
      "_internal",
      "ica-sidecar-aarch64-apple-darwin"
    ]
  }
}
